<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">


	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	
	<managed-bean>
		<managed-bean-name>menuBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.MenuBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>msg</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.MessagePropertiesBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean> 
	
	<managed-bean>
		<managed-bean-name>driveBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.DriveBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean> 
	
	<managed-bean>
		<managed-bean-name>driveListBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.DriveListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>hardDiskBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.HardDiskBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>hardDiskListBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.HardDiskListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>memoryBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.MemoryBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>memoryListBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.MemoryListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>motherboardBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.MotherboardBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>motherboardListBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.MotherboardListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>manufacturerBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.ManufacturerBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>processorBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.ProcessorBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>processorListBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.ProcessorListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>towerBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.TowerBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>towerListBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.TowerListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>videoCardBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.VideoCardBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>videoCardListBean</managed-bean-name>
		<managed-bean-class>br.com.gp.inventory.domain.bean.VideoCardListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

</faces-config>
